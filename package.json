{
    "name": "@netless/telebox-insider",
    "version": "0.2.46",
    "private": false,
    "description": "Window widget for Whiteboard",
    "license": "MIT",
    "sideEffects": [
      "*.css"
    ],
    "main": "./dist/telebox-insider.cjs.js",
    "module": "./dist/telebox-insider.es.js",
    "types": "./dist/index.d.ts",
    "files": [
      "dist",
      "src"
    ],
    "scripts": {
      "build": "vite build && yarn run types",
      "build-storybook": "build-storybook && node scripts/move-storybook.js",
      "build:dev": "vite build --mode development && yarn run types",
      "cleanup": "rimraf ./dist",
      "deploy-storybook": "yarn run build-storybook && node scripts/deploy-storybook.js",
      "lint": "lint-staged",
      "start": "vite ./playground --open --host",
      "storybook": "start-storybook -p 6006 --no-version-updates",
      "types": "dts -o dist/index.d.ts"
    },
    "devDependencies": {
      "@babel/plugin-proposal-class-properties": "^7.13.0",
      "@hyrious/dts": "^0.2.2",
      "@netless/eslint-plugin": "^1.1.2",
      "@rollup/plugin-commonjs": "^17.1.0",
      "@rollup/plugin-node-resolve": "^11.2.0",
      "@rollup/plugin-url": "^6.0.0",
      "@storybook/addon-actions": "^6.3.6",
      "@storybook/addon-essentials": "^6.3.6",
      "@storybook/addon-links": "^6.3.6",
      "@storybook/addons": "^6.3.6",
      "@storybook/react": "^6.3.6",
      "@storybook/theming": "^6.3.6",
      "@types/chance": "^1.1.1",
      "@types/faker": "^5.1.7",
      "@types/node": "^14.14.16",
      "@types/react": "^17.0.0",
      "@types/react-dom": "^17.0.0",
      "@types/shallowequal": "^1.1.1",
      "@typescript-eslint/eslint-plugin": "^4.11.0",
      "@typescript-eslint/parser": "^4.11.0",
      "chance": "^1.1.7",
      "cross-env": "^7.0.2",
      "eslint": "^7.31.0",
      "eslint-config-prettier": "^6.11.0",
      "eslint-config-react-app": "^6.0.0",
      "eslint-loader": "^4.0.2",
      "eslint-plugin-jsx-a11y": "^6.3.1",
      "eslint-plugin-prettier": "^3.1.4",
      "eslint-plugin-react": "^7.20.6",
      "eslint-plugin-react-app": "^6.2.2",
      "eventemitter3": "^4.0.7",
      "shallowequal": "^1.1.0",
      "stylefire": "^7.0.3",
      "faker": "^5.4.0",
      "fork-ts-checker-webpack-plugin": "^6.0.4",
      "lint-staged": "^10.5.4",
      "minimist": "^1.2.5",
      "prettier": "^2.0.5",
      "react": "^17.0.2",
      "react-dom": "^17.0.2",
      "react-refresh": "^0.8.3",
      "rimraf": "^3.0.0",
      "sass": "^1.36.0",
      "sass-loader": "^10.1.1",
      "side-effect-manager": "0.1.5",
      "typescript": "^5.4.5",
      "value-enhancer": "^0.0.8",
      "vite": "^2.4.4"
    }
  }
